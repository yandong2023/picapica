{
	"name": "picapicabooth",
	"version": "1.0.0",
	"description": "A modern web-based photo booth application that captures images using a webcam, applies filters, and generates a downloadable photo strip!",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
		"start:frontend": "cd frontend && PORT=3003 npm start",
		"start:backend": "cd backend && node server.js",
		"install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Agneswei/Picapica.git"
	},
	"keywords": [
		"photo-booth",
		"webcam",
		"photo-strip",
		"react",
		"nodejs"
	],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Agneswei/Picapica/issues"
	},
	"homepage": "https://picapicabooth.app",
	"dependencies": {
		"concurrently": "^8.2.2",
		"qrcode.react": "^4.2.0"
	}
}
